{
  "name": "ai-web-clients",
  "version": "1.0.0",
  "description": "AI Web Clients Monorepo",
  "main": "index.js",
  "scripts": {
    "graph": "nx graph",
    "version": "nx release version",
    "version:dry-run": "nx release version --dry-run",
    "release": "nx release",
    "release:dry-run": "nx release --dry-run",
    "publish": "nx release publish",
    "publish:dry-run": "nx release publish --dry-run",
    "dev:react": "nx serve react-integration-tests",
    "dev:mock:arh": "node apps/client-integration-tests/arh-mock-server.js",
    "dev:mock:lsc": "node apps/client-integration-tests/lightspeed-mock-server.js",
    "dev:mock": "concurrently \"npm run dev:mock:arh\" \"npm run dev:mock:lsc\"",
    "cypress:open": "cypress open --project apps/react-integration-tests-e2e",
    "cypress:run": "cypress run --project apps/react-integration-tests-e2e",
    "e2e": "concurrently \"npm run dev:react\" \"npm run dev:mock\" \"npm run cypress:run\"",
    "e2e:open": "npm run cypress:open"
  },
  "keywords": [
    "monorepo",
    "nx",
    "react",
    "typescript"
